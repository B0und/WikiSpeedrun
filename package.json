    "postcss": "^8.4.33",
    "postcss-import": "^16.0.0",
    "postcss-nesting": "^12.0.2",
    "prettier": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.1",
    "type-fest": "^4.10.1",
    "typescript": "5.3.3",
    "vite": "^5.0.12"
