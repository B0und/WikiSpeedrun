    "@tanstack/react-query": "^5.17.19",
    "@tanstack/react-query-devtools": "^5.17.21",
    "@types/dompurify": "^3.0.5",
    "clsx": "^2.1.0",
    "dompurify": "^3.0.9",
    "immer": "^10.0.3",
    "re-resizable": "^6.9.11",
    "react": "^18.2.0",
    "react-confetti-explosion": "^2.1.2",
    "react-dom": "^18.2.0",
